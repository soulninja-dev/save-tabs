{
  "manifest_version": 3,
  "name": "Tab Saver",
  "version": "1.0",
  "description": "Save tabs with tags and filter them",
  "permissions": ["tabs", "storage", "contextMenus"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
